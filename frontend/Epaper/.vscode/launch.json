{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug in Chrome",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug in Incognito Chrome",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "runtimeArgs": ["--incognito"]
    }
  ]
}
